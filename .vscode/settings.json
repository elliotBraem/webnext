{
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer",
		"editor.formatOnSave": true
	},
	"[typescript]": {
		"editor.defaultFormatter": "denoland.vscode-deno",
		"editor.formatOnSave": true
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "denoland.vscode-deno",
		"editor.formatOnSave": true
	},
	"editor.quickSuggestions": {
		"other": "on",
		"comments": true,
		"strings": "off"
	},
	"deno.enable": true,
	"deno.importMap": "configs/import-map.json",
	"deno.lint": true,
	"explorer.sortOrder": "filesFirst",
	"rust-analyzer.check.command": "clippy",
	"cSpell.words": [
		"Basestar",
		"bindgen",
		"borsh",
		"ceratosaur",
		"clippy",
		"deno",
		"embeddable",
		"Fractality",
		"ipapp",
		"jsxdev",
		"keychain",
		"roadmap",
		"rustup",
		"rxrust",
		"Stellarfox",
		"subroutes",
		"unocss"
	],
	"rust-analyzer.linkedProjects": [
		"./spaces/desktop/Cargo.toml"
	]
}