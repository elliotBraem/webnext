{
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer",
		"editor.formatOnSave": true
	},
	"[typescript]": {
		"editor.defaultFormatter": "rome.rome",
		"editor.formatOnSave": true
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "rome.rome",
		"editor.formatOnSave": true
	},
	"[javascript]": {
		"editor.defaultFormatter": "rome.rome",
	},
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features",
	},
	"editor.quickSuggestions": {
		"other": "on",
		"comments": true,
		"strings": true
	},
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.organizeImports.rome": true
	},
	"emmet.includeLanguages": {
		"postcss": "css"
	},
	"editor.semanticHighlighting.enabled": true,
	"deno.enable": true,
	"deno.importMap": "core/config/import-map.json",
	"deno.lint": true,
	"explorer.sortOrder": "filesFirst",
	"files.associations": {
		".asdfrc": "ini",
		"*.css": "tailwindcss"
	},
	"files.eol": "\n",
	"rust-analyzer.check.command": "clippy",
	"cSpell.words": [
		"Basestar",
		"bindgen",
		"borsh",
		"ceratosaur",
		"clippy",
		"deno",
		"embeddable",
		"Fractality",
		"ipapp",
		"jsxdev",
		"keychain",
		"libopus",
		"Megastructure",
		"mplayer",
		"Namebase",
		"nopep",
		"roadmap",
		"rustup",
		"rxrust",
		"Starfox",
		"subroutes",
		"SUNO",
		"superproject",
		"unocss",
		"webnext",
		"Attributify",
		"getr",
		"iconify",
		"nearfi",
		"openmoji",
		"pnpx",
		"unbundle",
		"unocss",
		"wendersonpires"
	],
	"[python]": {
		"editor.defaultFormatter": "ms-python.autopep8"
	},
	"python.formatting.provider": "none",
	"python.analysis.typeCheckingMode": "basic"
}