{
	"deno.enable": true,
	"deno.importMap": "configs/import-map.json",
	"deno.lint": true,
	"explorer.sortOrder": "filesFirst",
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer",
		"editor.formatOnSave": true
	},
	"[typescript]": {
		"editor.defaultFormatter": "denoland.vscode-deno",
		"editor.formatOnSave": true
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "denoland.vscode-deno",
		"editor.formatOnSave": true
	},
	"editor.quickSuggestions": {
		"other": "on",
		"comments": true,
		"strings": "off"
	},
	"cSpell.words": [
		"Basestar",
		"bindgen",
		"borsh",
		"ceratosaur",
		"deno",
		"ipapp",
		"jsxdev",
		"keychain",
		"roadmap",
		"rxrust",
		"subroutes",
		"unocss"
	]
}